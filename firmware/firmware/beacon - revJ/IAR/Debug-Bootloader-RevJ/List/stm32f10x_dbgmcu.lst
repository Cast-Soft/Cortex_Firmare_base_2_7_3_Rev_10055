     79          /**
     80            * @brief  Returns the device revision identifier.
     81            * @param  None
     82            * @retval Device revision identifier
     83            */
     84          uint32_t DBGMCU_GetREVID(void) {
     85              return (DBGMCU->IDCODE >> 16);
     86          }
     87          
     88          /**
     89            * @brief  Returns the device identifier.
     90            * @param  None
     91            * @retval Device identifier
     92            */
     93          uint32_t DBGMCU_GetDEVID(void) {
     94              return (DBGMCU->IDCODE & IDCODE_DEVID_MASK);
     95          }
